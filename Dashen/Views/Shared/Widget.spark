<h4>${heading}</h4>
<hr />
<div id="${id}">
  <script type="text/javascript">
    var fn = function () {
      $.ajax('${url}', {
        success: function (html) {
          $('#${id}').html(html);
        }
      });
    };
    $(document).ready(fn);
    setInterval(fn, 10000);
  </script>
</div>
