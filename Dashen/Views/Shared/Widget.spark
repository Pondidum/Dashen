<h4>${model.Heading}</h4>
<hr />
<div id="${model.ID}">
  <script type="text/javascript">
    var fn = function () {
      $.ajax('${model.Url}', {
        success: function (html) {
          $('#${model.ID}').html(html);
        }
      });
    };
    $(document).ready(fn);
    setInterval(fn, ${model.Interval});
  </script>
</div>
