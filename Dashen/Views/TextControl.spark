<viewdata model="Dashen.Controls.TextControlViewModel" />

<div id="${Model.ID}Content" class="text-center"></div>

<script type="text/javascript">

  var fn = function () {
    $.ajax({
      url: '${Model.UpdateUrl}'
    }).done(function (result) {
      $('#${Model.ID}Content').text(result.Content);
    });
  };

  $(document).ready(fn);
  setInterval(fn, ${Model.Interval});

</script>
