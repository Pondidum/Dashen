<viewdata model="Dashen.Controls.HtmlControlViewModel" />

<div id="${Model.ID}Html" class="text-center"></div>

<script type="text/javascript">

  var fn = function () {
    $.ajax({
      url: '${Model.UpdateUrl}'
    }).done(function (result) {
      $('#${Model.ID}Html').html(result.Html);
    });
  };

  $(document).ready(fn);
  setInterval(fn, ${Model.Interval});

</script>
